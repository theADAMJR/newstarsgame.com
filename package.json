    "next": "^13.4.4",
    "postcss": "8.4.24",
    "raw-loader": "^4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-pro-sidebar": "^1.1.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "gh-pages": "^5.0.0"
  }
